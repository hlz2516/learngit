@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    int ImageNum = ViewBag.ImageNum;
}

<div class="row">
    @for (int i = 0; i < ImageNum; i++)
    {
        <div class="col-3">
            <div class="card">
                <div class="card-body">
                    <img src="~/Image/upload@(i).jpg" alt="Alternate Text" />
                </div>
            </div>
        </div>
    }
</div>